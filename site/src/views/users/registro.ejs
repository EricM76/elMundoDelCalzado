<!DOCTYPE html>
<html lang="en">
    


 <!-- HEAD -->
<%- include('../partials/head') %> 
<!-- HEAD -->

<body>
    
  <!-- HEADER -->
  <%- include('../partials/header') %> 
  <!-- HEADER -->
  
      <main class="register">
            <div class="register1">
                <h1>REGISTRATE</h1>
                <hr>
                <form action="">

                    <%registro.map(element=>{%>
                        <div class="<%= element.class %> ">
                            <label for="<%= element.nombre %>"><%= element.nombre %> <strong class="obligatorio">*</strong></label>
                            <input type="text" name="<%= element.nombre %>" id="<%= element.nombre %>" placeholder="<%= element.nombre %>">
                        </div>
                        
                     <%}) %>
                     <small class="obligatorio">*Campo Obligatorio</small>
                     <div class="terminos">
                         <input type="checkbox" name="terminos" id="terminos">
                         <label for="terminos"> He leido y acepto los <a href="">t√©rminos y condiciones</a> y las <a href="">politicas de
                                 privacidad</a> del Mundo del Calzado.</label>
                     </div>
                     
                     
                     <%btnregistro.map(element=>{%>
                         <div class="<%=element.class%>">
                             <button type="<%=element.tipo %> " placeholder="<%=element.nombre %> "><%=element.nombre%></button>
                     
                         </div>
                         <p><%=element.parrafo %> </p>
                     <%})  %>
                     
                   
                </form>
            </div>
        </main>

            
        <!-- FOOTER -->
          <%- include('../partials/footer') %> 
        <!-- FOOTER -->

    </body>
</html>
